{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.0",
    "@ant-design/pro-layout": "^5.0.9",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.0.18",
    "axios": "^0.19.2",
    "dva": "^2.4.1",
    "lint-staged": "^10.0.7",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "qs": "^6.9.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "umi": "^3.0.18",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "path-to-regexp": "^6.1.0"
  }
}